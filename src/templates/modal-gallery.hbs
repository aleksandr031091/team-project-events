
<button type="button" class="lightbox__button" data-action="close-lightbox">âœ–</button>


<img src={{images.5.url}} alt='{{name}}' class='modal-img-gallery' />
<div class='modal-gallery'>

  <div class='poster-img'>
    <img class='modal-img' src={{images.5.url}} alt='{{name}}' />
  </div>
  <div class='modal-info'>
    <h3 class='info-title'>INFO</h3>
    <p class='info-text'>{{name}} </p>
    <h3 class='info-title'>WHEN</h3>
    <p class='info-text'>{{dates.start.localDate}}
      <br />
      {{dates.start.localTime}}
      ({{dates.timezone}})
    </p>
    <h3 class='info-title'>WHERE</h3>
    <p class='info-text'>{{_embedded.venues.0.city.name}},
      {{_embedded.venues.0.country.name}}
      <br />
      {{_embedded.venues.0.name}}
    </p>
    <h3 class='info-title'>WHO</h3>
    <p class='info-text'>{{_embedded.attractions.0.name}}</p>
    <h3 class='info-title'>PRICES</h3>
    {{#if priceRanges}}
    {{#each priceRanges}}
    <p class='info-text price-text'>
      <svg width="24" height="17" viewBox="0 0 24 17" fill="none" xmlns="http://www.w3.org/2000/svg">
<path d="M2.66668 0.833496L0 0.833496L0 16.8335H2.66668L2.66668 0.833496Z" fill="#0E0E0E"/>
<path d="M9.3737 0.833496L6.70703 0.833496L6.70703 16.8335H9.3737L9.3737 0.833496Z" fill="#0E0E0E"/>
<path d="M13.4142 0.833496L10.7476 0.833496L10.7476 16.8335H13.4142L13.4142 0.833496Z" fill="#0E0E0E"/>
<path d="M24.0001 0.833496L20.0405 0.833496L20.0405 16.8335H24.0001L24.0001 0.833496Z" fill="#0E0E0E"/>
<path d="M5.33346 0.833496L4.04053 0.833496L4.04053 16.8335H5.33346L5.33346 0.833496Z" fill="#0E0E0E"/>
<path d="M16 0.833496L14.707 0.833496L14.707 16.8335H16L16 0.833496Z" fill="#0E0E0E"/>
<path d="M18.6667 0.833496L17.3738 0.833496L17.3738 16.8335H18.6667L18.6667 0.833496Z" fill="#0E0E0E"/>
</svg>
      {{this.type}}
      {{this.min}}
      -
      {{this.max}}
      {{this.currency}}
    </p>
    <a href={{../url}} class="btn-buy-ticket link animate__animated animate__flash" target="_blank">BUY TICKETS</a>
    {{/each}}
    {{/if}}
    {{#unless priceRanges}}
    <p class='info-text prise-text'>Tickets are not yet available. Please, try again later.</p>
    {{/unless}}
  </div>
</div>
<button type='button' class='button button-more-info' data-action='more-info'>
  MORE FROM THIS AUTHOR
</button>